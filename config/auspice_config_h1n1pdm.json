{
  "title": "Real-time tracking of influenza A/H1N1pdm evolution and spread",
  "maintainers": [
    {"name": "Seattle Flu Study", "url": "https://seattleflu.org"}
  ],
  "build_url": "https://github.com/seattleflu/augur-build",
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "clade_membership",
      "title": "Clade",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    },
    {
      "key": "age_category",
      "title": "Age category",
      "type": "categorical"
    },
    {
      "key": "site_category",
      "title": "Site category",
      "type": "categorical"
    },
    {
      "key": "flu_shot",
      "title": "Vaccinated",
      "type": "categorical"
    },
    {
      "key": "cluster",
      "title": "Cluster",
      "type": "categorical"
    },
    {
      "key": "lbi",
      "title": "Local branching index",
      "type": "continuous"
    },
    {
      "key": "submitting_lab",
      "title": "Submitting lab",
      "type": "categorical"
    },
    {
      "key": "originating_lab",
      "title": "Originating lab",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "region"
